        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 60},
        "collapsed": false
      },
      {
        "id": 41,
        "title": "Quality Score Trend (7 Days)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "avg_over_time(((avg(rag_retrieval_precision_at_k{k_value=\"5\"}) + avg(rag_answer_faithfulness_score) + avg(rag_context_relevancy)) / 3)[7d:])",
            "legendFormat": "7-Day Avg Quality"
          },
          {
            "expr": "(avg(rag_retrieval_precision_at_k{k_value=\"5\"}) + avg(rag_answer_faithfulness_score) + avg(rag_context_relevancy)) / 3",
            "legendFormat": "Current Quality"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.5, "color": "yellow"},
                {"value": 0.7, "color": "green"}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 61}
      },
      {
        "id": 42,
        "title": "Critical Issues (Last Hour)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(rag_hallucination_detected_total{severity=\"severe\"}[1h]))",
            "legendFormat": "Severe Hallucinations"
          },
          {
            "expr": "sum(rag_answer_faithfulness_score < 0.5) or vector(0)",
            "legendFormat": "Low Faithfulness"
          },
          {
            "expr": "sum(rag_retrieval_precision_at_k{k_value=\"5\"} < 0.3) or vector(0)",
            "legendFormat": "Poor Retrieval"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 5, "color": "red"}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 61}
