{
  "dashboard": {
    "title": "RAG Quality Metrics",
    "tags": ["rag", "quality", "evaluation", "metrics"],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "üìä Quality Overview",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Overall Quality Score",
        "type": "stat",
        "targets": [
          {
            "expr": "(avg(rag_retrieval_precision_at_k{k_value=\"5\"}) + avg(rag_answer_faithfulness_score) + avg(rag_context_relevancy)) / 3",
            "legendFormat": "Quality Score"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.5, "color": "yellow"},
                {"value": 0.7, "color": "green"}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 1}
      },
      {
        "id": 3,
        "title": "Hallucination Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(rag_hallucination_detected_total[5m])",
            "legendFormat": "Hallucinations/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.01, "color": "yellow"},
                {"value": 0.05, "color": "red"}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 1}
      },
      {
        "id": 4,
        "title": "Retrieval Precision@5",
