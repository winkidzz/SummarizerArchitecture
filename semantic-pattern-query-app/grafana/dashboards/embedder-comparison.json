{
  "dashboard": {
    "title": "Embedder Comparison - Ollama vs Gemini",
    "tags": ["rag", "embedder", "comparison", "ollama", "gemini"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    },
    "panels": [
      {
        "id": 1,
        "title": "Embedder Usage Overview",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
        "collapsed": false
      },
      {
        "id": 2,
        "title": "Queries by Embedder Type",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (embedder_type) (increase(rag_queries_total[1h]))",
            "legendFormat": "{{embedder_type}}",
            "refId": "A",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 1},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        },
        "options": {
          "displayLabels": ["name", "percent"],
          "legend": {"display": true, "placement": "right"}
        }
      },
      {
        "id": 3,
        "title": "Query Rate by Embedder",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum by (embedder_type) (rate(rag_queries_total[1m]))",
            "legendFormat": "{{embedder_type}}",
            "refId": "A",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          }
        ],
        "gridPos": {"h": 8, "w": 16, "x": 8, "y": 1},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "reqps"
          }
        }
      },
      {
        "id": 4,
        "title": "Latency Comparison",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 9},
        "collapsed": false
      },
      {
        "id": 5,
        "title": "End-to-End Latency (P95) - Ollama vs Gemini",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(rag_query_duration_seconds_bucket{embedder_type=\"ollama\"}[1m])) by (le))",
            "legendFormat": "Ollama P95",
            "refId": "A",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(rag_query_duration_seconds_bucket{embedder_type=\"gemini\"}[1m])) by (le))",
            "legendFormat": "Gemini P95",
            "refId": "B",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(rag_query_duration_seconds_bucket{embedder_type=\"default\"}[1m])) by (le))",
            "legendFormat": "Default P95",
            "refId": "C",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 10},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "s"
          }
        }
      },
      {
        "id": 6,
        "title": "Average Latency Comparison",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(rate(rag_query_duration_seconds_sum{embedder_type=\"ollama\"}[5m]) / rate(rag_query_duration_seconds_count{embedder_type=\"ollama\"}[5m]))",
            "legendFormat": "Ollama Avg",
            "refId": "A",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          },
          {
            "expr": "avg(rate(rag_query_duration_seconds_sum{embedder_type=\"gemini\"}[5m]) / rate(rag_query_duration_seconds_count{embedder_type=\"gemini\"}[5m]))",
            "legendFormat": "Gemini Avg",
            "refId": "B",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 10},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 2, "color": "yellow"},
                {"value": 5, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Embedding Performance",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 18},
        "collapsed": false
      },
      {
        "id": 8,
        "title": "Embedding Duration by Embedder (P95)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(rag_embedding_duration_seconds_bucket{embedder_type=\"ollama\"}[1m])) by (le))",
            "legendFormat": "Ollama",
            "refId": "A",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(rag_embedding_duration_seconds_bucket{embedder_type=\"gemini\"}[1m])) by (le))",
            "legendFormat": "Gemini",
            "refId": "B",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 19},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "s"
          }
        }
      },
      {
        "id": 9,
        "title": "Embedding Duration Comparison (Avg)",
        "type": "bargauge",
        "targets": [
          {
            "expr": "avg(rate(rag_embedding_duration_seconds_sum{embedder_type=\"ollama\"}[5m]) / rate(rag_embedding_duration_seconds_count{embedder_type=\"ollama\"}[5m]))",
            "legendFormat": "Ollama",
            "refId": "A",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          },
          {
            "expr": "avg(rate(rag_embedding_duration_seconds_sum{embedder_type=\"gemini\"}[5m]) / rate(rag_embedding_duration_seconds_count{embedder_type=\"gemini\"}[5m]))",
            "legendFormat": "Gemini",
            "refId": "B",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 19},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "unit": "s",
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 0.5, "color": "yellow"},
                {"value": 1, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient"
        }
      },
      {
        "id": 10,
        "title": "Quality Metrics",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 27},
        "collapsed": false
      },
      {
        "id": 11,
        "title": "Retrieval Quality by Embedder",
        "type": "timeseries",
        "targets": [
          {
            "expr": "avg by (embedder_type) (rag_retrieval_docs{embedder_type=~\"ollama|gemini\"})",
            "legendFormat": "{{embedder_type}} - Avg Docs",
            "refId": "A",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },
      {
        "id": 12,
        "title": "Success Rate by Embedder",
        "type": "stat",
        "targets": [
          {
            "expr": "100 * sum(rate(rag_queries_total{status=\"success\",embedder_type=\"ollama\"}[5m])) / sum(rate(rag_queries_total{embedder_type=\"ollama\"}[5m]))",
            "legendFormat": "Ollama",
            "refId": "A",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          },
          {
            "expr": "100 * sum(rate(rag_queries_total{status=\"success\",embedder_type=\"gemini\"}[5m])) / sum(rate(rag_queries_total{embedder_type=\"gemini\"}[5m]))",
            "legendFormat": "Gemini",
            "refId": "B",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 95, "color": "yellow"},
                {"value": 99, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 13,
        "title": "Cost Analysis",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 36},
        "collapsed": false
      },
      {
        "id": 14,
        "title": "Query Volume by Embedder (Hourly)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(rag_queries_total{embedder_type=\"ollama\"}[1h]))",
            "legendFormat": "Ollama (free)",
            "refId": "A",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          },
          {
            "expr": "sum(increase(rag_queries_total{embedder_type=\"gemini\"}[1h]))",
            "legendFormat": "Gemini (paid)",
            "refId": "B",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          }
        ],
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 37},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 15,
        "title": "Embedder Selection Trend",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum by (embedder_type) (rate(rag_queries_total[5m]))",
            "legendFormat": "{{embedder_type}}",
            "refId": "A",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          }
        ],
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 37},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "reqps"
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "bottom"}
        }
      }
    ],
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 1,
    "links": [],
    "liveNow": true,
    "style": "dark",
    "templating": {"list": []},
    "annotations": {"list": []}
  }
}
