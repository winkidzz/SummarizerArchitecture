{
  "dashboard": {
    "title": "RAG System - Real-Time Telemetry",
    "tags": ["rag", "telemetry", "realtime"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "5s",
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "panels": [
      {
        "id": 1,
        "title": "Total Queries",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rag_queries_total)",
            "legendFormat": "Total",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            }
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },
      {
        "id": 2,
        "title": "Query Rate (per second)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(rag_queries_total[1m]))",
            "legendFormat": "Queries/sec",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            }
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 6, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "reqps"
          }
        }
      },
      {
        "id": 3,
        "title": "Success vs Error Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(rag_queries_total{status=\"success\"}[1m]))",
            "legendFormat": "Success",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            }
          },
          {
            "expr": "sum(rate(rag_queries_total{status=\"error\"}[1m]))",
            "legendFormat": "Error",
            "refId": "B",
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            }
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 4},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "reqps"
          }
        }
      },
      {
        "id": 4,
        "title": "Query Duration (P50, P95, P99)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(rag_query_duration_seconds_bucket[1m])) by (le))",
            "legendFormat": "P50",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            }
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(rag_query_duration_seconds_bucket[1m])) by (le))",
            "legendFormat": "P95",
            "refId": "B",
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            }
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(rag_query_duration_seconds_bucket[1m])) by (le))",
            "legendFormat": "P99",
            "refId": "C",
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            }
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 6, "y": 4},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "s"
          }
        }
      },
      {
        "id": 5,
        "title": "Retrieval Documents",
        "type": "timeseries",
        "targets": [
          {
            "expr": "avg(rag_retrieval_docs)",
            "legendFormat": "Avg Docs Retrieved",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            }
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 12},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },
      {
        "id": 6,
        "title": "Retrieval Duration",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(rag_retrieval_duration_seconds_bucket[1m])) by (le))",
            "legendFormat": "P95 Retrieval Time",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            }
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 12},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "s"
          }
        }
      },
      {
        "id": 7,
        "title": "Generation Duration",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(rag_generation_duration_seconds_bucket[1m])) by (le))",
            "legendFormat": "P95 Generation Time",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            }
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 20},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "s"
          }
        }
      },
      {
        "id": 8,
        "title": "Answer Length & Citations",
        "type": "timeseries",
        "targets": [
          {
            "expr": "avg(rag_answer_length)",
            "legendFormat": "Avg Answer Length",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            }
          },
          {
            "expr": "avg(rag_citation_count)",
            "legendFormat": "Avg Citations",
            "refId": "B",
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            }
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 20},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },
      {
        "id": 9,
        "title": "Embedding Duration",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(rag_embedding_duration_seconds_bucket[1m])) by (le))",
            "legendFormat": "P95 Embedding Time",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            }
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 28},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "s"
          }
        }
      },
      {
        "id": 10,
        "title": "Cache Hit Rate",
        "type": "gauge",
        "targets": [
          {
            "expr": "sum(rate(rag_cache_hits_total[1m])) / (sum(rate(rag_cache_hits_total[1m])) + sum(rate(rag_cache_misses_total[1m])))",
            "legendFormat": "Hit Rate",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "PBFA97CFB590B2093"
            }
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 28},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 0.5, "color": "yellow"},
                {"value": 0.8, "color": "green"}
              ]
            }
          }
        }
      }
    ],
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 0,
    "links": [],
    "liveNow": true,
    "style": "dark",
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    }
  }
}

