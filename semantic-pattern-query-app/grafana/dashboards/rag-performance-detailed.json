{
  "dashboard": {
    "title": "RAG Performance - Detailed Analytics",
    "tags": ["rag", "performance", "analytics"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-30m",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    },
    "panels": [
      {
        "id": 1,
        "title": "Query Performance Overview",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
        "collapsed": false
      },
      {
        "id": 2,
        "title": "Total Queries (Success vs Error)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rag_queries_total{status=\"success\"})",
            "legendFormat": "Success",
            "refId": "A",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          },
          {
            "expr": "sum(rag_queries_total{status=\"error\"})",
            "legendFormat": "Error",
            "refId": "B",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          }
        ],
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 1},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 1, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Success Rate %",
        "type": "gauge",
        "targets": [
          {
            "expr": "100 * sum(rate(rag_queries_total{status=\"success\"}[5m])) / sum(rate(rag_queries_total[5m]))",
            "legendFormat": "Success Rate",
            "refId": "A",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          }
        ],
        "gridPos": {"h": 6, "w": 6, "x": 6, "y": 1},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 90, "color": "yellow"},
                {"value": 99, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Query Rate (QPS)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(rag_queries_total[1m]))",
            "legendFormat": "Total QPS",
            "refId": "A",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          },
          {
            "expr": "sum(rate(rag_queries_total{status=\"success\"}[1m]))",
            "legendFormat": "Success QPS",
            "refId": "B",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          },
          {
            "expr": "sum(rate(rag_queries_total{status=\"error\"}[1m]))",
            "legendFormat": "Error QPS",
            "refId": "C",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          }
        ],
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 1},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "reqps"
          }
        }
      },
      {
        "id": 5,
        "title": "Query Latency",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 7},
        "collapsed": false
      },
      {
        "id": 6,
        "title": "End-to-End Latency (P50, P95, P99)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(rag_query_duration_seconds_bucket[1m])) by (le))",
            "legendFormat": "P50",
            "refId": "A",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(rag_query_duration_seconds_bucket[1m])) by (le))",
            "legendFormat": "P95",
            "refId": "B",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(rag_query_duration_seconds_bucket[1m])) by (le))",
            "legendFormat": "P99",
            "refId": "C",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "s"
          }
        }
      },
      {
        "id": 7,
        "title": "Average Latency by Component",
        "type": "timeseries",
        "targets": [
          {
            "expr": "avg(rate(rag_query_duration_seconds_sum[1m]) / rate(rag_query_duration_seconds_count[1m]))",
            "legendFormat": "Total",
            "refId": "A",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          },
          {
            "expr": "avg(rate(rag_retrieval_duration_seconds_sum[1m]) / rate(rag_retrieval_duration_seconds_count[1m]))",
            "legendFormat": "Retrieval",
            "refId": "B",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          },
          {
            "expr": "avg(rate(rag_generation_duration_seconds_sum[1m]) / rate(rag_generation_duration_seconds_count[1m]))",
            "legendFormat": "Generation",
            "refId": "C",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          },
          {
            "expr": "avg(rate(rag_embedding_duration_seconds_sum[1m]) / rate(rag_embedding_duration_seconds_count[1m]))",
            "legendFormat": "Embedding",
            "refId": "D",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "s"
          }
        }
      },
      {
        "id": 8,
        "title": "Retrieval Performance",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 16},
        "collapsed": false
      },
      {
        "id": 9,
        "title": "Documents Retrieved (Distribution)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "avg(rag_retrieval_docs)",
            "legendFormat": "Avg Docs",
            "refId": "A",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(rag_retrieval_docs_bucket[1m])) by (le))",
            "legendFormat": "P95 Docs",
            "refId": "B",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 17},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },
      {
        "id": 10,
        "title": "Retrieval Duration by Stage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(rag_retrieval_duration_seconds_bucket{stage=\"initial\"}[1m])) by (le))",
            "legendFormat": "Initial (P95)",
            "refId": "A",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(rag_retrieval_duration_seconds_bucket{stage=\"final\"}[1m])) by (le))",
            "legendFormat": "Final (P95)",
            "refId": "B",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 17},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "s"
          }
        }
      },
      {
        "id": 11,
        "title": "Retriever Type Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (retriever_type) (increase(rag_retrieval_docs_count[5m]))",
            "legendFormat": "{{retriever_type}}",
            "refId": "A",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 17},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },
      {
        "id": 12,
        "title": "Generation & Answer Quality",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 25},
        "collapsed": false
      },
      {
        "id": 13,
        "title": "Answer Length (chars)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "avg(rag_answer_length)",
            "legendFormat": "Avg Length",
            "refId": "A",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(rag_answer_length_bucket[1m])) by (le))",
            "legendFormat": "P95 Length",
            "refId": "B",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 26},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },
      {
        "id": 14,
        "title": "Citation Count",
        "type": "timeseries",
        "targets": [
          {
            "expr": "avg(rag_citation_count)",
            "legendFormat": "Avg Citations",
            "refId": "A",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(rag_citation_count_bucket[1m])) by (le))",
            "legendFormat": "P95 Citations",
            "refId": "B",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 26},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },
      {
        "id": 15,
        "title": "Generation Duration (P95)",
        "type": "gauge",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(rag_generation_duration_seconds_bucket[1m])) by (le))",
            "legendFormat": "P95 Time",
            "refId": "A",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 26},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "unit": "s",
            "min": 0,
            "max": 10,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "green"},
                {"value": 5, "color": "yellow"},
                {"value": 8, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 16,
        "title": "Caching Performance",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 34},
        "collapsed": false
      },
      {
        "id": 17,
        "title": "Cache Hit Rate",
        "type": "gauge",
        "targets": [
          {
            "expr": "100 * sum(rate(rag_cache_hits_total[5m])) / (sum(rate(rag_cache_hits_total[5m])) + sum(rate(rag_cache_misses_total[5m])))",
            "legendFormat": "Hit Rate %",
            "refId": "A",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 35},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 50, "color": "yellow"},
                {"value": 80, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 18,
        "title": "Cache Hits vs Misses (Rate)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(rag_cache_hits_total[1m]))",
            "legendFormat": "Hits/sec",
            "refId": "A",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          },
          {
            "expr": "sum(rate(rag_cache_misses_total[1m]))",
            "legendFormat": "Misses/sec",
            "refId": "B",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          }
        ],
        "gridPos": {"h": 8, "w": 9, "x": 6, "y": 35},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "reqps"
          }
        }
      },
      {
        "id": 19,
        "title": "Queries by Cache Status",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum by (cache_status) (rate(rag_queries_total[1m]))",
            "legendFormat": "{{cache_status}}",
            "refId": "A",
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"}
          }
        ],
        "gridPos": {"h": 8, "w": 9, "x": 15, "y": 35},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "reqps"
          }
        }
      }
    ],
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 1,
    "links": [],
    "liveNow": true,
    "style": "dark",
    "templating": {"list": []},
    "annotations": {"list": []}
  }
}
